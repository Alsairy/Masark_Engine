{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "SecretKey": "ThisIsAVeryLongSecretKeyForJWTTokenGenerationThatMeetsMinimumRequirements123456789",
    "Issuer": "Masark.API",
    "Audience": "Masark.Client",
    "ExpirationMinutes": 60
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=masark.db",
    "Redis": "localhost:6379"
  },
  "Cdn": {
    "Enabled": false,
    "BaseUrl": "",
    "ApiKey": "",
    "CacheMaxAge": 31536000,
    "ImageOptimization": {
      "Enabled": true,
      "DefaultFormat": "webp",
      "QualitySettings": {
        "High": 90,
        "Medium": 75,
        "Low": 60
      }
    }
  },
  "Caching": {
    "RedisConnectionString": "localhost:6379",
    "InstanceName": "MasarkEngine",
    "DefaultExpirationMinutes": 30,
    "MaxCacheSize": 1000,
    "EnableDistributedCache": true,
    "EnableMemoryCache": true,
    "EnableCompression": true
  },
  "AllowedHosts": "*"
}
